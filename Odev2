--film tablosunda bulunan tüm sütunlardaki verileri replacement cost değeri 12.99'Dan büyük eşit ve 16.99 küçük olma koşulu
SELECT * FROM film 
WHERE length BETWEEN 12.99 AND 16.99;

--actor tablosunda bulunan first_name ve last_name sütunlardaki verileri first_name 'Penelope' veya 'Nick' veya 'Ed' değerleri olması koşulu
SELECT first_name, last_name FROM actor 
WHERE IN('Penelope', 'Nick', 'Ed');

--film tablosunda bulunan tüm sütunlardaki verileri rental_rate 0.99, 2.99,4.99 ve replacement_cost 12.99, 15.99,28.99 olması koşulu
SELECT * FROM film 
WHERE (rental_rate IN(0.99,2.99,4.99)) AND (replacement_cost IN(12.99,15.99,28.99));
