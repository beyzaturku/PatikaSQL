--film tablosunda bulunan ve film ismi 'n' karakteri ile biten en uzun 5 filmin sıralanması
SELECT * FROM film 
WHERE title LIKE '%n'
ORDER BY length DESC
LIMIT 5;

--film tablosunda bulunan ve film isni 'n' karakteri ile biten en uzun ikinci 5 film
SELECT* FROM film
WHERE title LIKE '%n'
ORDER BY length 
OFFSET 5
LIMIT 5;

--customer tablosunda bulunan last_name sütununa göre azalan yapılan sıralamada store_id 1 olmak koşuluyla ilk 4 verinin sıralanması
SELECT * FROM customer
WHERE store_id=1
ORDER BY last_name DESC
LIMIT 4;
