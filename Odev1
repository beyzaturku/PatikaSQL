--film tablosunda bulunan title ve description sütunlarındaki verilerin sıralanması
--SELECT title, description FROM film 

--film tablosunda bulunan verilerin film uzunluğu 60'dan büyük ve 75'ten küçük olma koşulu
SELECT * FROM film WHERE length>60 AND length<75;

--film tablosunda bulunan verilerin rental_rate 0.99 ve replacement_cost 12.99 veya 28.99 olma koşulu
SELECT title, description FROM film WHERE rental_rate=0.99 AND (replacement_cost=12.99 OR replacement_cost=28.99);

--customer tablosunda bulunan first_name sütunundaki değeri 'MARY' olan müşterinin last_name sütunundaki değeri?
SELECT last_name FROM customer WHERE first_name = 'Mary';

--film tablosundaki uzunluğu 50'ten büyük olmayıp aynı zamanda rental_rate değeri 2.99 veya 4.99 olmayan verilerin sıralanması;
SELECT * FROM film Where length<50 AND NOT rental_rate IN (2.99, 4.99);
